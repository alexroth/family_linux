<h1>Listing family_start_points</h1>

<table>
  <thead>
    <tr>
      <th>Family descrip</th>
      <th>Membid</th>
      <th>Comment</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @family_start_points.each do |family_start_point| %>
      <tr>
        <td><%= family_start_point.family_descrip %></td>
        <td><%= family_start_point.membid %></td>
        <td><%= family_start_point.comment %></td>
        <td><%= family_start_point.created_at %></td>
        <td><%= family_start_point.updated_at %></td>
        <td><%= link_to 'Show', family_start_point %></td>
        <td><%= link_to 'Edit', edit_family_start_point_path(family_start_point) %></td>
        <td><%= link_to 'Destroy', family_start_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

